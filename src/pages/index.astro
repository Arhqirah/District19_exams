---

import Layout from "../layouts/Layout.astro";
import Hero from "../components/Hero_Small.astro";
import Product from "../components/Product.astro";
import ProductSection from "../components/ProductSection.astro";
import Section from "../components/Section.astro";
import Button from "../components/Button.astro";




---

<Layout imageVariant="stor" title="Falafel Factory" Image="/heroImg/Hero_Home.png">
   
    <ProductSection sectionText="Vores populære rætter" sectionVariant="3col">
        <Product 
        imgSrc="retter/freestylepita.webp" 
        imgAlt="En spicy carrot pita" 
        headerText="Spicy Carrot"
        description="Fetaost, cherrytomater, valnødder, mynte, hummus, persillesalat og tahindressing" 
        buttonVariant="primary" 
        buttonText="Tilføj ordre"
        price="80">
    </Product>
    <Product 
        imgSrc="retter/kukupita.webp" 
        imgAlt="Kuku Pita" 
        headerText="Kuku Pita" 
        description="Kartoffelomelet med krydderurter, hummus, persillesalat og tahindressing"
        buttonVariant="primary" 
        buttonText="Tilføj ordre"
        price="80">
    </Product>
    <Product 
        imgSrc="retter/rootspita.webp" 
        imgAlt="Roots Pita" 
        headerText="Roots Pita" 
        description="Rødbede falafler, tahin, rødkålssalat, dukkah og tahindressing"
        buttonVariant="primary" 
        buttonText="Tilføj ordre"
        price="70">
    </Product>
    <Button buttonVariant="tertiary" buttonText="SE HELE MENUEN"/>
   
</Section>
<Section
    sectionHeader="VI TILBYDER CATERING TIL DIT NÆSTE EVENT"
    sectionVariant="2col"
    sectionBackground="sectionWhite">
    <div class="sectionImg">
        <img src="#" alt="Et billede">
    </div>
    <div>
        <p>Medbring den fulde Falafel Factory oplevelse hvor som helst! Vi fokuserer på kvalitet af ingredienser, som vi gerne vil dele med dig og dine gæster.</p>
    </div>
    <span></span>
</Section>
</Layout/>
    
