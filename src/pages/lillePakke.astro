---
import Layout from "../layouts/Layout.astro";
import TilbehorForm from "../components/tilbehorForm.astro";
import Section from "../components/Section.astro";

const {Image} = Astro.props;
const {imageVariant} = Astro.props;
---

<Layout imageVariant="lille" Image="/heroImg/Hero_Catering.png"  title="Falafel Factory - CATERING">
    <Section
    sectionVariant="1col"
    imgVariant="imgRight"
    imgType="imgCenter"
    span="span3"
    sectionBackground="sectionWhite"
    sectionTitle="Catering til dit næste event?"
    sectionText="Medbring den fulde Falafel Factory oplevelse hvor som helst. Vores catering tilbydes til alle typer af begivenheder, og vores retter består af samme gode kvalitet som i vores butik. Vælg imellem vores mindre frokost pakke eller vores all exclusive pakke. Du er også altid velkommen til at tage kontakt til os, så vi i fælleskab kan sammensætte den perfekte menu, efter netop dine  og dine gæsters behov."
    sectionImg=""
    sectionAlt=""
    buttonVariant="4th" 
    buttonText="Læs mere">
        
    </Section>
<TilbehorForm/>
</Layout>