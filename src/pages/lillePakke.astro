---
import Layout from "../layouts/Layout.astro";
import TilbehorForm from "../components/tilbehorForm.astro";
import Pakker from "../components/Pakker2.astro";

const {Image} = Astro.props;
const {imageVariant} = Astro.props;
const showTitle = true;
---

<Layout imageVariant="lille" Image="/heroImg/Hero_Catering.png"  title="Falafel Factory - CATERING" showTitle={showTitle} heroTitle="Den Lille Pakke">
    <Pakker
    sectionBackground=""
    sectionVariant=""
    PakkeTitle="120,- PR. KUVERT"
    PakkeTekst="Vores lille cateringpakke er uden dessert. Den er perfekt til dem, der ønsker en lækker smagsoplevelse til en mere overkommelig pris."
    ForretTekst="Frisklavet hummus og babaganoush serveret med sprøde grøntsagsstænger og luftige pitabrød."
    Hovedret1="Vores berømte og saftige falafelkugler serveret med frisklavet tzatziki og hummus."
    Hovedret2="Grillede halloumi-oste med vores hemmelige krydderiblanding."
    Hovedret3="Sprøde, farverige grøntsagsspyd."
    DessertTekst="Vores signatur baklava og frisk frugtsalat."
    imgType=""
    imgSrc="\Pakke_Lille_Hoej.webp"
    imgAlt=""
    ></Pakker>
</Layout>

